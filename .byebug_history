exit
@appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M")
@appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M%")
@appointment.appointment_datetime.strftime("%B %d, %Y at %H")
@appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M%p")
@appointment.appointment_datetime.strftime("%B %d, %Y at %I:%M%p")
